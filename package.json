{
  "name": "jwt-auth-lab5",
  "version": "1.0.0",
  "description": "JWT Authentication with CI/CD",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles --testTimeout=30000",
    "test:unit": "NODE_ENV=test jest app/test/unit --forceExit --testTimeout=10000",
    "test:integration": "NODE_ENV=test jest app/test/integration --forceExit --testTimeout=30000",
    "test:coverage": "NODE_ENV=test jest --coverage --forceExit --testTimeout=30000"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  }
}
